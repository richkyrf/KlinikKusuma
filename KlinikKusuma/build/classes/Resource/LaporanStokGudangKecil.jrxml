	<queryString>
		<![CDATA[SELECT a.`IdBarang`, `NamaBarang`, `StokAwal`, `Penerimaan`, `Perawatan`, `Penjualan`, `Penyesuaian`, `StokAkhir` FROM (
(SELECT `IdBarang`, `NamaBarang`, `IdJenisBarang` FROM `tbmbarang` WHERE 1 ) a
LEFT JOIN
(SELECT `IdBarang`, SUM(`Jumlah`) as 'Penerimaan' FROM `tbpermintaanstok` WHERE `Tanggal` BETWEEN $P{TglAwal} AND $P{TglAkhir} GROUP BY `IdBarang`) b
    ON a.`IdBarang`=b.`IdBarang`
LEFT JOIN
(SELECT `IdObat`, SUM(`Jumlah`) as 'Perawatan' FROM `tbbillingobat`a JOIN `tbbilling`b ON a.`NoBilling`=b.`NoBilling` WHERE `Tanggal` BETWEEN $P{TglAwal} AND $P{TglAkhir} GROUP BY `IdObat` ) c
    ON a.`IdBarang`=c.`IdObat`
LEFT JOIN
(SELECT `IdObat` as 'IdBarang', SUM(`Jumlah`) as 'Penjualan' FROM `tbbillingobat`a JOIN `tbbilling`b ON a.`NoBilling`=b.`NoBilling` WHERE `Tanggal` BETWEEN $P{TglAwal} AND $P{TglAkhir} GROUP BY `IdBarang`) d
    ON a.`IdBarang`=d.`IdBarang`
LEFT JOIN
(SELECT `IdBarang`, SUM(`Jumlah`) as 'Penyesuaian' FROM `tbpenyesuaianstok` WHERE `Tanggal` BETWEEN $P{TglAwal} AND $P{TglAkhir} GROUP BY `IdBarang`) e
	ON a.`IdBarang`=e.`IdBarang`
LEFT JOIN
    (SELECT `IdBarang`, IF(SUM(`StokAwal`)=0,null,SUM(`StokAwal`)) as 'StokAwal' FROM (
SELECT `IdBarang`, `NamaBarang`, 0 as 'StokAwal' FROM `tbmbarang` WHERE 1
UNION ALL
SELECT `IdBarang`, null as 'NamaBarang', `Jumlah` as 'StokAwal' FROM `tbpermintaanstok` WHERE `Tanggal` < $P{TglAwal}
UNION ALL
SELECT `IdObat` as 'IdBarang', null as 'NamaBarang', `Jumlah`*-1 as 'StokAwal' FROM `tbbillingobat`a JOIN `tbbilling`b ON a.`NoBilling`=b.`NoBilling` WHERE `Tanggal` < $P{TglAwal}
UNION ALL
SELECT `IdObat`, null as 'NamaBarang', `Jumlah`*-1 as 'StokAwal' FROM `tbbillingobat`a JOIN `tbbilling`b ON a.`NoBilling`=b.`NoBilling` WHERE `Tanggal` < $P{TglAwal}
UNION ALL
SELECT `IdBarang`, null as 'NamaBarang', `Jumlah`*-1 as 'StokAwal' FROM `tbpenyesuaianstok` WHERE `Tanggal` < $P{TglAwal}
) t1 GROUP BY `IdBarang`) f
    ON a.`IdBarang`=f.`IdBarang`
LEFT JOIN
    (SELECT `IdBarang`, IF(SUM(`StokAkhir`)=0,null,SUM(`StokAkhir`)) as 'StokAkhir' FROM (
SELECT `IdBarang`, `NamaBarang`, 0 as 'StokAkhir' FROM `tbmbarang` WHERE 1
UNION ALL
SELECT `IdBarang`, null as 'NamaBarang', `Jumlah` as 'StokAkhir' FROM `tbpermintaanstok` WHERE `Tanggal` <= $P{TglAkhir}
UNION ALL
SELECT `IdObat`, null as 'NamaBarang', `Jumlah`*-1 as 'StokAkhir' FROM `tbbillingobat`a JOIN `tbbilling`b ON a.`NoBilling`=b.`NoBilling` WHERE `Tanggal` <= $P{TglAkhir}
UNION ALL
SELECT `IdObat` as 'IdBarang', null as 'NamaBarang', `Jumlah`*-1 as 'StokAkhir' FROM `tbbillingobat`a JOIN `tbbilling`b ON a.`NoBilling`=b.`NoBilling` WHERE `Tanggal` <= $P{TglAkhir}
UNION ALL
SELECT `IdBarang`, null as 'NamaBarang', `Jumlah`*-1 as 'StokAkhir' FROM `tbpenyesuaianstok` WHERE `Tanggal` <= $P{TglAkhir}
) t1 GROUP BY `IdBarang`) g
    ON a.`IdBarang`=g.`IdBarang`
) ORDER BY a.`IdJenisBarang`, a.`NamaBarang`]]>
	</queryString>
